      },
      {
        "type": "Text",
        "id": "text-strategy-4-intro",
        "props": {
          "text": "When subtracting numbers close to 10, 20, 30, etc., it's easier to subtract the round number and then add back the difference."
        }
      },
      {
        "type": "StepByStepSolution",
        "id": "solution-subtract-near",
        "props": {
          "title": "Example: 85 - 29",
          "steps": [
            {
              "id": "step-1",
              "description": "Subtract 30 instead of 29",
              "work": "\\begin{align*} 85 - 30 = 55 \\end{align*}",
              "explanation": "30 is close to 29 and easier to work with"
            },
            {
              "id": "step-2",
              "description": "Add back 1",
              "work": "\\begin{align*} 55 + 1 = 56 \\end{align*}",
              "explanation": "We took away too much by 1 (since we subtracted 30 not 29), so add it back"
            },
            {
              "id": "step-3",
              "description": "Final answer",
              "work": "\\begin{align*} 85 - 29 = 56 \\end{align*}",
              "explanation": "Much easier than traditional subtraction!"
            }
          ]
        }
