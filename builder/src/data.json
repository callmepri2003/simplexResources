      {
        "type": "Heading",
        "id": "heading-strategy-3",
        "props": {
          "heading": "Strategy 3: The 11 Times Table Trick"
        }
      },
      {
        "type": "Text",
        "id": "text-strategy-3-intro",
        "props": {
          "text": "Multiplying by 11 is super easy once you know the pattern! For two-digit numbers, add the two digits together and put the answer in the middle."
        }
      },
      {
        "type": "StepByStepSolution",
        "id": "solution-times-11",
        "props": {
          "title": "Example: 23 × 11",
          "steps": [
            {
              "id": "step-1",
              "description": "Write the first digit",
              "work": "\\begin{align*} 2\\_ \\_ \\end{align*}",
              "explanation": "Start with the first digit of 23, which is 2"
            },
            {
              "id": "step-2",
              "description": "Add the two digits",
              "work": "\\begin{align*} 2 + 3 = 5 \\end{align*}",
              "explanation": "Add the two digits of 23 together"
            },
            {
              "id": "step-3",
              "description": "Put the sum in the middle",
              "work": "\\begin{align*} 2\\underline{5}\\_ \\end{align*}",
              "explanation": "The 5 goes in the middle"
            },
            {
              "id": "step-4",
              "description": "Write the last digit",
              "work": "\\begin{align*} 253 \\end{align*}",
              "explanation": "End with the last digit of 23, which is 3. So 23 × 11 = 253!"
            }
          ]
        }
      },
      {
        "type": "CommonMisconception",
        "id": "misconception-11-trick",
        "props": {
          "misconception": "The trick doesn't work if the two digits add up to 10 or more",
          "reality": "It still works, but you need to carry the 1",
          "explanation": "For example: 67 × 11. Add 6 + 7 = 13. Write down the 3 and carry the 1 to the first digit. So you get 7, then 3, then 7, but you add the carried 1 to the first 6 to get 7. The answer is 737."
        }
      },
