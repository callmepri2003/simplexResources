        "props": {
          "heading": "Strategy 5: Percentage Shortcuts"
        }
      },
      {
        "type": "Text",
        "id": "text-strategy-5-intro",
        "props": {
          "text": "Finding percentages can be really quick with these tricks. Remember: 10% is the same as dividing by 10, and you can build other percentages from there!"
        }
      },
      {
        "type": "ExplanationBox",
        "id": "explanation-percentage-tricks",
        "props": {
          "title": "Quick Percentage Tricks",
          "content": [
            {
              "type": "ExplanationBoxList",
              "id": "list-percentage",
              "props": {
                "items": [
                  "10% = Divide by 10 (move decimal point left one place)",
                  "5% = Half of 10%",
                  "25% = Quarter of the number (divide by 4)",
                  "50% = Half of the number (divide by 2)",
                  "20% = Double 10%",
                  "15% = 10% + 5%"
                ]
              }
            }
          ]
        }
      },
      {
        "type": "StepByStepSolution",
        "id": "solution-percentage",
        "props": {
          "title": "Example: Find 15% of $80",
          "steps": [
            {
              "id": "step-1",
              "description": "Find 10% first",
              "work": "\\begin{align*} 10\\% \\text{ of } \\$80 = \\$8 \\end{align*}",
              "explanation": "Divide 80 by 10 to get 8"
            },
            {
              "id": "step-2",
              "description": "Find 5% (half of 10%)",
              "work": "\\begin{align*} 5\\% \\text{ of } \\$80 = \\$4 \\end{align*}",
              "explanation": "Half of $8 is $4"
            },
            {
              "id": "step-3",
              "description": "Add them together",
              "work": "\\begin{align*} \\$8 + \\$4 = \\$12 \\end{align*}",
              "explanation": "10% + 5% = 15%, so $8 + $4 = $12"
            }
          ]
        }
